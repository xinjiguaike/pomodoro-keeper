            <SolidColorBrush x:Key="WhiteForegroundThemeBrush" Color="#FFFFFFFF" />
            <SolidColorBrush x:Key="BlackForegroundThemeBrush" Color="#FF000000" />
            <SolidColorBrush x:Key="GrayForegroundThemeBrush" Color="#FF333434" />
            <SolidColorBrush x:Key="LightGreenForegroundThemeBrush" Color="#FF308E6A" />
            <SolidColorBrush x:Key="DarkGreenForegroundThemeBrush" Color="#FF14553B" />
            <!-- Common Border Style -->
            <SolidColorBrush x:Key="LightGreenBorderThemeBrush" Color="#FF308E6A" />
            <SolidColorBrush x:Key="DarkGreenBorderThemeBrush" Color="#FF14553B" />
